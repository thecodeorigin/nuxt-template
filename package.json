{
  "name": "@thecodeorigin/nuxt",
  "type": "module",
  "version": "2.0.0-beta.21",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "packageManager": "pnpm@9.12.1",
  "author": "thecodeorigin <contact@thecodeorigin.com>",
  "homepage": "https://github.com/thecodeorigin/nuxt-template",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/thecodeorigin/nuxt-template.git",
    "directory": ""
  },
  "exports": {
    ".": "./nuxt.config.ts",
    "./utils/*": "./app/utils/*.ts",
    "./server/utils/*": "./server/utils/*.ts",
    "./schemas": "./server/db/schemas/index.ts",
    "./schemas/*": "./server/db/schemas/*.ts"
  },
  "main": "./nuxt.config.ts",
  "scripts": {
    "with-env": "doppler run -- pnpm",
    "dev": "jiti ./scripts/serve.ts",
    "build": "nuxt build",
    "prepack": "jiti ./scripts/prepack.ts",
    "lint": "eslint . --fix",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "jiti ./scripts/prepare.ts",
    "test": "vitest run",
    "start": "node .output/server/index.mjs",
    "cleanup": "rimraf .nuxt .output dist node_modules areas",
    "stripe:seed": "jiti ./scripts/generate-stripe.ts --esmResolve",
    "i18n:extract": "vue-i18n-extract report --vueFiles \"./app/**/*.?(js|ts|vue)\" --languageFiles \"./app/assets/locale/*.?(json)\" --add --separator '->'",
    "db:preview": "drizzle-kit studio --port 3001",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit generate && drizzle-kit migrate",
    "db:clear": "rimraf ./docker/postgres",
    "db:start": "docker compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "pnpm db:stop && pnpm db:clear && pnpm db:start --force-recreate",
    "db:seed": "jiti ./server/db/seeds/index.ts",
    "gettext:extract": "vue-gettext-extract",
    "gettext:compile": "vue-gettext-compile"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.10",
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/s3-request-presigner": "^3.717.0",
    "@casl/ability": "6.7.2",
    "@casl/vue": "2.2.2",
    "@firebase/app-types": "^0.9.3",
    "@iconify-json/heroicons": "^1.2.2",
    "@iconify-json/lucide": "^1.2.20",
    "@iconify-json/simple-icons": "^1.2.17",
    "@logto/nuxt": "^1.1.5",
    "@nuxt/content": "^2.13.4",
    "@nuxt/eslint": "^0.7.4",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/image": "^1.8.1",
    "@nuxt/ui-pro": "^1.6.0",
    "@nuxtjs/device": "^3.2.4",
    "@nuxtjs/i18n": "^8.5.5",
    "@nuxtjs/robots": "^5.0.1",
    "@nuxtjs/seo": "^2.0.2",
    "@nuxtjs/sitemap": "^7.0.0",
    "@payos/node": "^1.0.10",
    "@pinia/nuxt": "^0.5.1",
    "@sentry/nuxt": "^8.36.0",
    "@sindresorhus/is": "6.3.0",
    "@thecodeorigin/eslint-config": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/mapbox-gl": "3.1.0",
    "@types/node": "20.12.7",
    "@types/nodemailer": "^6.4.16",
    "@types/pdfmake": "^0.2.9",
    "@types/uuid": "^8.3.3",
    "@types/webfontloader": "1.6.38",
    "@unovis/ts": "^1.5.0",
    "@unovis/vue": "^1.5.0",
    "@vueuse/core": "^11.1.0",
    "@vueuse/math": "^11.1.0",
    "@vueuse/nuxt": "^12.2.0",
    "bcrypt": "^5.1.1",
    "cookie-es": "1.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.1",
    "drizzle-orm": "^0.38.3",
    "drizzle-zod": "^0.6.1",
    "eslint": "^9.17.0",
    "execa": "^9.5.2",
    "firebase": "^11.1.0",
    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.2.0",
    "lodash-es": "^4.17.21",
    "mongodb": "^6.12.0",
    "nodemailer": "^6.9.16",
    "nuxt": "^3.15.0",
    "nuxt-gtag": "^3.0.2",
    "nuxt-link-checker": "^4.0.2",
    "nuxt-module-hotjar": "^1.3.2",
    "nuxt-nodemailer": "^1.1.2",
    "nuxt-og-image": "^3.0.6",
    "nuxt-schema-org": "^4.0.4",
    "nuxt-security": "^2.1.4",
    "nuxt-vuefire": "^1.0.3",
    "postgres": "^3.4.5",
    "stripe": "^17.5.0",
    "ts-pattern": "^5.6.0",
    "type-fest": "4.31.0",
    "v-calendar": "^3.1.2",
    "vnpay": "^1.6.1",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^2.2.0",
    "vuefire": "^3.2.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "vercel": "^39.2.2"
  },
  "resolutions": {
    "nuxt": "3.14.1592"
  }
}
